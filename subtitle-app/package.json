{
  "name": "subtitle-generator-app",
  "version": "1.0.0",
  "description": "React frontend and FastAPI backend for subtitle generation",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && venv\\Scripts\\python main.py",
    "frontend": "cd frontend && npm start",
    "install-all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {},
  "keywords": ["subtitle", "transcription", "translation", "elevenlabs", "react", "fastapi"],
  "author": "",
  "license": "ISC"
}